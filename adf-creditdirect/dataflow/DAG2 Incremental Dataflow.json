{
	"name": "DAG2 Incremental Dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DestinationDataset_7hb",
						"type": "DatasetReference"
					},
					"name": "MySQLSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DestinationDataset_7hb",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empNo as string,",
				"          birthDate as string,",
				"          firstName as string,",
				"          lastName as string,",
				"          gender as string,",
				"          hireDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 1000,",
				"     ignoreNoFilesFound: false,",
				"     waterMarkColumn: 'emp_no',",
				"     mode: 'read') ~> MySQLSource",
				"MySQLSource derive(gender = iif(gender == \"M\", \"Male\", iif(gender == \"F\", \"Female\", \"Not Defined\"))) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          empNo as string,",
				"          birthDate as string,",
				"          firstName as string,",
				"          lastName as string,",
				"          gender as string,",
				"          hireDate as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}