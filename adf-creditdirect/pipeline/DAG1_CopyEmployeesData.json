{
	"name": "DAG1_CopyEmployeesData",
	"properties": {
		"description": "Pipeline to copy all employees table to CSV file in Azure Datalake",
		"activities": [
			{
				"name": "Copy_AllEmployees",
				"description": "copies all Employee Data into a staging blob file ready for sync to Big Query.\nCan also be written to Big Query API directly",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "employees"
					},
					{
						"name": "Destination",
						"value": "dataverse//cd-employees.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "emp_no",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "empNo",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "birth_date",
									"type": "DateTime",
									"physicalType": "date"
								},
								"sink": {
									"name": "birthDate",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "first_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "firstName",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "last_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "lastName",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "gender",
									"type": "String",
									"physicalType": "enum"
								},
								"sink": {
									"name": "gender",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "hire_date",
									"type": "DateTime",
									"physicalType": "date"
								},
								"sink": {
									"name": "hireDate",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_MySQL",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_7hb",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}